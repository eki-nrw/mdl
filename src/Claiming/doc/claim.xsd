<?xml version="1.0"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="https://www.w3schools.com" xmlns="https://www.w3schools.com" elementFormDefault="qualified">

<xs:element name="claim:definition">
	<xs:complexType>
		<xs:sequence>
      		<xs:element name="name" type="xs:string"/>
      		<xs:element name="type" type="xs:string"/>
      		<xs:element name="participants" maxOccurs="unbounded">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="participant">
							<xs:complexType>
								<xs:attribute name="name" type="xs:string"/> 						
								<xs:sequence>
									<xs:element name="name" type="xs:string"/>
									<xs:element name="class" type="xs:string"/>
								</xs:sequence>
							</xs:complexType>
			    	</xs:sequence>
				</xs:complexType>
      		</xs:element>
    	</xs:sequence>
	</xs:complexType>
</xs:element>

</xs:schema>
